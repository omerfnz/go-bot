# Go-SERP-Bot Environment Configuration

# Application Settings
HEADLESS=true
WORKERS=5
INTERVAL=300

# Logging
LOG_LEVEL=info
LOG_FILE=logs/serp-bot.log

# Timeouts (seconds)
PAGE_TIMEOUT=30
SEARCH_TIMEOUT=15

# Retry Settings
MAX_RETRIES=3
RETRY_DELAY=5

# Browser Settings
USER_AGENT_ROTATION=true
STEALTH_MODE=true

# Proxy Settings (optional)
PROXY_USERNAME=
PROXY_PASSWORD=
PROXY_ROTATION_STRATEGY=round-robin

# Stats
STATS_FILE=data/stats.json

# Development
DEBUG=false

